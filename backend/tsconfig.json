{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["webpack-env"]
  },
  "include": ["src/**/*", "sequelize.config.js"],
  "exclude": ["node_modules", "dist"]
}
